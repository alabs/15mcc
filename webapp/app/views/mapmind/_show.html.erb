  <div id="mindmap-wrapper" class="span9">
    <div id="mindmap">
    </div><!-- mindmap-demo -->
  </div><!-- mindmap-demo-wrapper -->

  <script id="tmpl-node" type="text/template">
    <div id="{{= id }}" class="node" style="left: {{= pos_left }}; top: {{= pos_top }}">
      <i class="icon icon-remove"></i>
      <span class="node_name">{{= label }}</span>
    </div>
  </script>

  <script id="tmpl-infobox" type="text/template">
    <div id="infobox" class="well">
      <div class="span3">
        <h3 id="node-label">{{= label }}</h3>
        <a href="#" id="infobox-close"><i class="icon-remove"></i></a>
      </div>
      <h4>Etiquetas</h4>
      <ul id="node-tags"></ul>
    </div>
  </script>

  <script id="tmpl-infobox-tag" type="text/template">
    <li><a href="/tags/show/{{= label }}">{{= label }}</a></li>
  </script>
